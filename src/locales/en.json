    "safety": {
        "title": "Safety & Community Policy",
        "intro": "To keep GThaiLover safe and welcoming for everyone, all users must follow the rules below.<br />Using our platform means you accept all terms in this policy.",
        "sections": {
            "1": {
                "title": "1. Prohibited Content and Behavior",
                "sub1": {
                    "title": "1.1 Sexual or Explicit Content",
                    "text1": "Pornographic or explicit sexual content is strictly prohibited",
                    "text2": "Do not share, request, or discuss explicit sexual acts"
                },
                "sub2": {
                    "title": "1.2 Drugs and Illegal Substances",
                    "text1": "Do not promote, sell, or request drugs or any illegal substances"
                },
                "sub3": {
                    "title": "1.3 Minors (Under 18)",
                    "text1": "No profiles, photos, or content involving anyone under the age of 18",
                    "text2": "Do not pretend to be under 18"
                },
                "sub4": {
                    "title": "1.4 Sensitive Personal Information",
                    "intro": "Do not share or request:",
                    "list": [
                        "Credit card information",
                        "Bank account details",
                        "Passwords or login information",
                        "ID card numbers",
                        "Any personal or sensitive information"
                    ]
                },
                "sub5": {
                    "title": "1.5 Impersonation and Use of Someone Else’s Photos",
                    "text1": "Do not use someone else's photos",
                    "text2": "Do not impersonate another person or pretend to be a real individual",
                    "text3": "AI-generated photos that resemble real people without permission are not allowed"
                },
                "sub6": {
                    "title": "1.6 Financial Scams / Investment / Gambling",
                    "text1": "Do not promote investment schemes, financial assistance, or money transfers",
                    "text2": "No gambling or betting activities",
                    "text3": "Do not solicit investments, donations, or money"
                },
                "sub7": {
                    "title": "1.7 Illegal Activities",
                    "text1": "Any illegal activities under applicable law are strictly prohibited"
                }
            },
            "2": {
                "title": "2. Warning and Ban Policy",
                "intro": "To maintain community safety:",
                "sub1": {
                    "title": "2.1 General Violations",
                    "text": "Users who receive <1>3 warnings</1> and fail to correct the issue will be automatically banned"
                },
                "sub2": {
                    "title": "2.2 Invalid / Prohibited Photos",
                    "text": "Users who receive <1>10 photo warnings</1> (feed or profile) will be automatically banned"
                }
            },
            "3": {
                "title": "3. Legal Responsibility",
                "text1": "GThaiLover is not involved in or supportive of any illegal user actions",
                "text2": "Any illegal behavior conducted through the platform is solely the user's responsibility",
                "text3": "By using GThaiLover, users acknowledge and accept this responsibility"
            },
            "4": {
                "title": "4. Agreement",
                "text": "Creating an account or using GThaiLover means you agree to follow all the above rules. Violations may result in warnings, content removal, or permanent account suspension."
            }
        }

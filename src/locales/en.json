        "title": "Community & Safety Policy",
        "intro": "To keep GThaiLover a safe and enjoyable place for everyone, all users must follow these rules.<br />By using this platform, you agree to all policies.",
        "sections": {
            "1": {
                "title": "1. Prohibited Content & Behavior",
                "sub1": {
                    "title": "1.1 Sexual or Explicit Content",
                    "text1": "Posting pornographic, explicit, or obscene content is prohibited.",
                    "text2": "Do not share, request, or discuss sexual activities explicitly."
                },
                "sub2": {
                    "title": "1.2 Drugs and Illegal Substances",
                    "text1": "Do not post or encourage the buying, selling, or use of illegal drugs."
                },
                "sub3": {
                    "title": "1.3 Strictly No Minors (Under 18)",
                    "text1": "No images, profiles, or content involving minors.",
                    "text2": "Do not claim to be under 18."
                },
                "sub4": {
                    "title": "1.4 Sensitive Personal Information",
                    "intro": "Do not share or request the following:",
                    "list": [
                        "Credit card information",
                        "Bank account details",
                        "Passwords or login credentials",
                        "Identification numbers",
                        "Other sensitive personal data"
                    ]
                },
                "sub5": {
                    "title": "1.5 Impersonation & Using Others’ Photos",
                    "text1": "Do not use someone else's photo.",
                    "text2": "Do not impersonate or claim to be another real person.",
                    "text3": "Do not use AI-generated images of real people without permission."
                },
                "sub6": {
                    "title": "1.6 Financial Scams / Investment / Gambling",
                    "text1": "No promotions of investments, money transfers, or financial requests.",
                    "text2": "No gambling or inviting others to gamble.",
                    "text3": "Do not request money, investments, or donations."
                },
                "sub7": {
                    "title": "1.7 Illegal Activities",
                    "text1": "All illegal activities are strictly prohibited."
                }
            },
            "2": {
                "title": "2. Warning & Ban System",
                "intro": "To maintain community safety:",
                "sub1": {
                    "title": "2.1 General Violations",
                    "text": "If a user receives <1>3 warnings</1> and continues violating rules, the account will be automatically banned."
                },
                "sub2": {
                    "title": "2.2 Inappropriate / Forbidden Photos",
                    "text": "If a user receives <1>10 image warnings</1> (from feed or profile), the account will be automatically banned."
                }
            },
            "3": {
                "title": "3. Legal Responsibility",
                "text1": "GThaiLover does not support or engage in any illegal activities by users.",
                "text2": "Any illegal action is solely the user's responsibility.",
                "text3": "Using this website means you accept this responsibility."
            },
            "4": {
                "title": "4. Consent",
                "text": "By signing up or using GThaiLover, you agree to all rules. Violations may result in warnings, content removal, or permanent ban."
            }
